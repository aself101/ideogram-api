{
  "name": "ideogram-api",
  "version": "1.1.0",
  "description": "Node.js wrapper for Ideogram API - Image generation, editing, remixing, and manipulation",
  "type": "module",
  "main": "dist/api.js",
  "types": "dist/api.d.ts",
  "bin": {
    "ideogram": "dist/cli.js"
  },
  "exports": {
    ".": {
      "import": "./dist/api.js",
      "types": "./dist/api.d.ts"
    },
    "./api": {
      "import": "./dist/api.js",
      "types": "./dist/api.d.ts"
    },
    "./utils": {
      "import": "./dist/utils.js",
      "types": "./dist/utils.d.ts"
    },
    "./config": {
      "import": "./dist/config.js",
      "types": "./dist/config.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/cli.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "semantic-release": "semantic-release",
    "prepublishOnly": "npm run build",
    "ideogram": "tsx src/cli.ts",
    "ideogram:help": "tsx src/cli.ts --help",
    "ideogram:examples": "tsx src/cli.ts --examples",
    "ideogram:generate": "tsx src/cli.ts generate",
    "ideogram:edit": "tsx src/cli.ts edit",
    "ideogram:remix": "tsx src/cli.ts remix",
    "ideogram:reframe": "tsx src/cli.ts reframe",
    "ideogram:replace-background": "tsx src/cli.ts replace-background",
    "ideogram:upscale": "tsx src/cli.ts upscale",
    "ideogram:describe": "tsx src/cli.ts describe",
    "ideogram:list-styles": "tsx src/cli.ts list-style-presets",
    "ideogram:list-ratios": "tsx src/cli.ts list-aspect-ratios",
    "ideogram:list-resolutions": "tsx src/cli.ts list-resolutions"
  },
  "keywords": [
    "ideogram",
    "ai",
    "image-generation",
    "text-to-image",
    "image-to-image",
    "image-editing",
    "upscale",
    "remix",
    "reframe",
    "typescript"
  ],
  "author": "img-gen Contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aself101/ideogram-api.git"
  },
  "bugs": {
    "url": "https://github.com/aself101/ideogram-api/issues"
  },
  "homepage": "https://github.com/aself101/ideogram-api#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "commander": "^11.1.0",
    "dotenv": "^16.3.1",
    "form-data": "^4.0.0",
    "image-size": "^2.0.2",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.6",
    "@semantic-release/npm": "^12.0.2",
    "@semantic-release/release-notes-generator": "^14.1.0",
    "@types/node": "^24.10.1",
    "@vitest/coverage-v8": "^4.0.10",
    "@vitest/ui": "^4.0.10",
    "semantic-release": "^24.2.9",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.10"
  }
}
