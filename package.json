{
  "name": "ideogram-api",
  "version": "1.0.2",
  "description": "Node.js wrapper for Ideogram API - Image generation, editing, remixing, and manipulation",
  "type": "module",
  "main": "api.js",
  "bin": {
    "ideogram": "./cli.js"
  },
  "exports": {
    ".": "./api.js",
    "./api": "./api.js",
    "./utils": "./utils.js",
    "./config": "./config.js"
  },
  "files": [
    "api.js",
    "cli.js",
    "utils.js",
    "config.js",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "semantic-release": "semantic-release",
    "ideogram": "node cli.js",
    "ideogram:help": "node cli.js --help",
    "ideogram:examples": "node cli.js --examples",
    "ideogram:generate": "node cli.js generate",
    "ideogram:edit": "node cli.js edit",
    "ideogram:remix": "node cli.js remix",
    "ideogram:reframe": "node cli.js reframe",
    "ideogram:replace-background": "node cli.js replace-background",
    "ideogram:upscale": "node cli.js upscale",
    "ideogram:describe": "node cli.js describe",
    "ideogram:list-styles": "node cli.js list-style-presets",
    "ideogram:list-ratios": "node cli.js list-aspect-ratios",
    "ideogram:list-resolutions": "node cli.js list-resolutions"
  },
  "keywords": [
    "ideogram",
    "ai",
    "image-generation",
    "text-to-image",
    "image-to-image",
    "image-editing",
    "upscale",
    "remix",
    "reframe"
  ],
  "author": "img-gen Contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aself101/ideogram-api.git"
  },
  "bugs": {
    "url": "https://github.com/aself101/ideogram-api/issues"
  },
  "homepage": "https://github.com/aself101/ideogram-api#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "commander": "^11.1.0",
    "dotenv": "^16.3.1",
    "form-data": "^4.0.0",
    "image-size": "^2.0.2",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.6",
    "@semantic-release/npm": "^12.0.2",
    "@semantic-release/release-notes-generator": "^14.1.0",
    "@vitest/coverage-v8": "^1.1.0",
    "@vitest/ui": "^1.1.0",
    "semantic-release": "^24.2.9",
    "vitest": "^1.1.0"
  }
}
